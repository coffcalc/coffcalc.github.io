{
    "compilerOptions": {
        "rootDir": "src",
        "outDir": "lib",
        "target": "es2015",
        "module": "esnext",
        "lib": ["dom", "es5"],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true
    }
}
